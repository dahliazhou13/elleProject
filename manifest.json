{    "name": "Ducking: Green Shopping Tool",   
     "version": "1.0",    
     "description": "An extension that helps you make online shopping greener and reward you with points!",    
     "manifest_version": 2,
     "icons":{
        "16":"ducking.png",
        "48":"ducking.png",
        "128": "ducking.png"
         },
     "browser_action": {
        "default_icon": "ducking duck.png",
        "default_popup": "popup.html",
        "default_title": "Ducking"
    },
    "background": {
        "scripts": ["backgroundData.js"],
        "persistent": false
      },
    "web_accessible_resources":[
      "./table_1.json"
    ],
    "permissions": [
        "tabs"
      ]
}